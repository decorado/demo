{
  "name": "decora-browser-lib-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build.lib": "ng build @decora/browser-lib-ui && npm run copy-bootstrap && npm run copy-themes && npm run copy-lib-readmes && npm run copy-bootstrap-readmes",
    "copy-bootstrap": "cpx \"./projects/decora/browser-lib-ui/bootstrap/**/*\" \"./dist/decora/browser-lib-ui/bootstrap/\"",
    "copy-themes": "cpx \"./projects/decora/browser-lib-ui/src/lib/**/*theming.scss\" \"./dist/decora/browser-lib-ui/bootstrap/theming/\"",
    "copy-lib-readmes": "cpx \"./projects/decora/browser-lib-ui/src/lib/**/*.md\" \"./dist/decora/browser-lib-ui/lib/\"",
    "copy-bootstrap-readmes": "cpx \"./projects/decora/browser-lib-ui/bootstrap/**/*.md\" \"./dist/decora/browser-lib-ui/bootstrap/\"",
    "mock.server": "nodemon ./../decora-mock-server/mock-server.js",
    "start.mock": "concurrently --kill-others \"npm run mock.server\" \"npm run start\"",
    "start.mock-aot": "concurrently --kill-others \"npm run mock.server\" \"ng s --aot\"",
    "build.gh": "npm run build.lib && ng build --prod --base-href=https://decorado.github.io --output-path=docs && npm run create-404",
    "create-404": "cp docs/index.html docs/404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/cdk": "^7.0.0-rc.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/material": "^7.0.0-rc.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "@ngu/carousel": "^1.4.8",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@swimlane/ngx-datatable": "^13.1.0",
    "angular2-prettyjson": "^3.0.1",
    "core-js": "^2.5.4",
    "cpx": "^1.5.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "ngx-markdown": "^6.1.0",
    "prismjs": "^1.15.0",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.6",
    "nodemon": "^1.18.3",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.9.1",
    "typescript": "^3.1.6"
  }
}
